<Window x:Class="integerExample.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:integerExample"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <TextBox x:Name="txtInput" />
        <Button Content="Click me"  x:Name="btnProcess" Click="btnProcess_Click" />
        <Button Content="Example 2" x:Name="btnExample2" Click="btnExample2_Click" />
        <TextBlock x:Name="txtOutput" />
    </StackPanel>
</Window>
